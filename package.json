{
  "name": "lifecraft-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "echo 'Supabase setup - no additional steps needed'",
    "gp": "bash scripts/git-push.sh",
    "push": "bash scripts/git-push.sh",
    "watch:push": "bash scripts/auto-commit-push.sh",
    "hooks:setup": "git config core.hooksPath .githooks && chmod +x .githooks/post-commit scripts/*.sh || true",
    "migrate": "node scripts/migration-runner.js",
    "migrate:export": "node scripts/export-current-data.js",
    "migrate:postgresql": "node scripts/migrate-data-to-postgresql.js",
    "migrate:verify": "node scripts/verify-migration.js",
    "migrate:rollback": "node scripts/rollback-to-sqlite.js",
    "setup:collaborator": "bash scripts/setup-collaborator-env.sh",
    "setup:env": "node scripts/setup-env-from-secrets.js",
    "env:encrypt": "node scripts/simple-encrypt-env.js encrypt",
    "env:decrypt": "node scripts/simple-encrypt-env.js decrypt",
    "admin:setup": "node scripts/setup-collaborator-admin.js",
    "admin:promote": "node scripts/promote-user-to-admin.js",
    "admin:env-setup": "node scripts/setup-admin-from-env.js",
    "migrate:enhanced": "node scripts/migrate-to-enhanced-schema.js",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9",
    "autoprefixer": "^10.4.20",
    "chart.js": "^4.5.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "d3": "^7.9.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "openai": "^5.15.0",
    "postcss": "^8.4.49",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.1.2",
    "tailwindcss": "^3.4.16",
    "typescript": "^5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/d3": "^7.4.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.0"
  }
}
